{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "client",
      "cwd": "client",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "toolArgs": [
        "--no-enable-impeller"
      ]
    },
    {
      "name": "client (web)",
      "cwd": "client",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "lib/main.dart",
      "toolArgs": [
        "--wasm",
        "-d",
        "chrome",
        "--web-port",
        "10000"
      ]
    },
    {
      "name": "client (web - no wasm)",
      "cwd": "client",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "lib/main.dart",
      "toolArgs": [
        "-d",
        "chrome",
        "--web-port",
        "10000"
      ]
    },
    {
      "name": "client (profile mode)",
      "cwd": "client",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "lib/main.dart",
    },
    {
      "name": "client (release mode)",
      "cwd": "client",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "lib/main.dart",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Server",
      "program": "${workspaceFolder}/server/dist/index.js",
      "preLaunchTask": "npm: build - server",
      "cwd": "${workspaceFolder}/server",
      "sourceMaps": true,
      "skipFiles": [
        "<node_internals>/**",
        "node_moduled/**/*"
      ],
      "envFile": "${workspaceFolder}/server/.env",
      "console": "integratedTerminal"
    },
    {
      "name": "siq_file",
      "type": "dart",
      "request": "launch",
      "program": "./client/packages/siq_file/bin/siq_file.dart",
      "args": [
        "json",
        "../../../test.siq"
      ]
    }
  ]
}