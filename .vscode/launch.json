    "version": "0.2.0",
    "configurations": [
        {
            "name": "client",
            "cwd": "client",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-renderer",
                "html"
            ]
        },
        {
            "name": "client (profile mode)",
            "cwd": "client",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "client (release mode)",
            "cwd": "client",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "siq_file",
            "type": "dart",
            "request": "launch",
            "program": "./client/packages/siq_file/bin/siq_file.dart",
            "args": [
                "parse",
                "../../../test.siq"
            ]
        },
    ]
}